package com;

public class SinglyLinkedList {
	
	class Node{
		int data;
		Node next;
		
		Node(int data){
			this.data=data;
			this.next=null;
			
		}
		
 public static void main(String[] args) {
			
			Node head=new Node(10);
			head.next=new Node(20);
			head.next.next=new Node(30);
			
			printList(head);
		} 
		
	public static void printList(Node node) {
		while(node!=null) {
			System.out.print(node.data+" ");
			node=node.next;
		}
	}
		
	}

}
